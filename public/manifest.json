{
    "name": "Melange",
    "description": "A toolkit for web browsing utilizing the GPT-3 text engine through OpenAI",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
       "activeTab",
       "scripting",
       "contextMenus",
       "storage"
    ],
    "options_ui": {
      "page": "options.html"
    },
    "background": {
       "service_worker": "background.js"
    }
 }